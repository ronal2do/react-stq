.gradient-front {
    background: linear-gradient(90deg, #2e3d76, #763287, #cf064e);
    background-size: 400% 100%;
    background-position: 0 0;
    -webkit-animation: animateGradient 10s linear infinite;
    animation: animateGradient 10s linear infinite;
    height: 100%;
    position: relative
}

.gradient-front .front-arrow-wrap {
    position: absolute;
    z-index: 100;
    bottom: .1em;
    display: block;
    width: 100%;
    height: 4.5rem;
    padding-top: 2.25rem
}

.gradient-front .front-arrow-wrap:hover .bar:after,
.gradient-front .front-arrow-wrap:hover .bar:before {
    background: transparent;
    border: 3px solid #fff;
    box-shadow: 0 0 20px 5px hsla(0, 0%, 100%, .6)
}

.gradient-front .arrow-wrap {
    width: 60px;
    height: 40px;
    font-size: .5em;
    display: block;
    will-change: transform;
    -webkit-animation: bounce 1.5s ease-in-out infinite;
    animation: bounce 1.5s ease-in-out infinite;
    margin-left: calc(50% - 20px);
    -webkit-transform: translateZ(0);
    transform: translateZ(0)
}

.gradient-front .arrow-wrap .bar {
    position: initial
}

.gradient-front .arrow-wrap .bar:after,
.gradient-front .arrow-wrap .bar:before {
    content: '';
    display: block;
    position: absolute;
    height: 5px;
    width: 35px;
    border-radius: 1px;
    background: #fff;
    cursor: pointer
}

.gradient-front .arrow-wrap .bar:before {
    -webkit-transform: rotate(45deg);
    transform: rotate(45deg);
    background: #fff
}

.gradient-front .arrow-wrap .bar:after {
    -webkit-transform: rotate(-45deg);
    transform: rotate(-45deg);
    right: 3px
}