    border-radius: 18px;
    -webkit-animation: pulse ease-in-out 1.5s infinite;
    animation: pulse ease-in-out 1.5s infinite;
    position: relative
}

.label:after {
    content: '';
    position: absolute;
    top: 50%;
    left: 50%;
    width: 36px;
    height: 36px;
    margin-left: -18px;
    margin-top: -18px;
    box-shadow: 0 0 0 1px #fff;
    border-radius: 50%;
    -webkit-animation: sonar 1.5s linear infinite;
    animation: sonar 1.5s linear infinite
}

@-webkit-keyframes pulse {
    40% {
        -webkit-transform: scale(1.3);
        transform: scale(1.3)
    }
    to {
        -webkit-transform: scale(1);
        transform: scale(1)
    }
}

@keyframes pulse {
    40% {
        -webkit-transform: scale(1.3);
        transform: scale(1.3)
    }
    to {
        -webkit-transform: scale(1);
        transform: scale(1)
    }
}

@-webkit-keyframes sonar {
    0% {
        -webkit-transform: scale3d(.5, .5, .5);
        transform: scale3d(.5, .5, .5);
        opacity: 1
    }
    50%,
    to {
        -webkit-transform: scaleX(1);
        transform: scaleX(1);
        opacity: 0
    }
}

@keyframes sonar {
    0% {
        -webkit-transform: scale3d(.5, .5, .5);
        transform: scale3d(.5, .5, .5);
